{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"id": "http://axway.com/schemas/codeblockmetadata.json",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"schemaVersion": {
			"description": "The codeblock metadata schema version.",
			"type": "string",
			"enum": [
				"1"
			]
		},
		"name": {
			"type": "string",
			"description": "The function name of the codeblock."
		},
		"description": {
			"type": "string",
			"description": "The description of the codeblock."
		},
		"path": {
			"type": "string",
			"description": "The path to the codeblock source file."
		},
		"parameter": {
			"$ref": "http://json-schema.org/draft-06/schema#",
			"description": "An object that describes a set of parameter inputs into the code block."
		},
		"outputs": {
			"description": "Describes the code block response.",
			"additionalProperties": {
				"$ref": "http://axway.com/schemas/flowschema.json#/output-spec"
			}
		}
	},
	"required": [
		"schemaVersion",
		"name",
		"path",
		"parameter",
		"outputs"
	]
}
